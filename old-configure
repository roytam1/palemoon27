  enableval="$enable_system_hunspell"
  if test "$enableval" = "yes"; then
    MOZ_NATIVE_HUNSPELL=1 
  elif test "$enableval" = "no"; then
    :
  else
    { echo "configure: error: Option, system-hunspell, does not take an argument ($enableval)." 1>&2; echo "configure: error: Option, system-hunspell, does not take an argument ($enableval)." 1>&5; exit 1; }
  fi
