  echo "$ac_t""yes" 1>&6
  MOZ_NATIVE_WEBP=1 MOZ_WEBP_LIBS="-lwebp"
else
  echo "$ac_t""no" 1>&6
MOZ_NATIVE_WEBP= MOZ_WEBP_CFLAGS= MOZ_WEBP_LIBS=
fi

fi
