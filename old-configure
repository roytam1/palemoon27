if test -n "$MOZ_RUST"; then
  echo $ac_n "checking rustc target sse2 support""... $ac_c" 1>&6
echo "configure:9781: checking rustc target sse2 support" >&5
  case "$OS_ARCH:$CPU_ARCH" in
    WINNT:x86)
