    case "$JAVA" in
    /*)
      tmp_DIRNAME=`dirname "$JAVA"`
      tmp_BASENAME=`basename "$JAVA"`
      tmp_PWD=`cd "$tmp_DIRNAME" && pwd -W`
      JAVA="$tmp_PWD/$tmp_BASENAME"
      if test -e "$JAVA.exe"; then
        JAVA="$JAVA.exe"
      fi
    esac
  fi

