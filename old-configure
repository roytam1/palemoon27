# Check whether --enable-profilelocking or --disable-profilelocking was given.
if test "${enable_profilelocking+set}" = set; then
  enableval="$enable_profilelocking"
  if test "$enableval" = "no"; then
    MOZ_PROFILELOCKING=
  elif test "$enableval" = "yes"; then
    MOZ_PROFILELOCKING=1 
  else
    { echo "configure: error: Option, profilelocking, does not take an argument ($enableval)." 1>&2; echo "configure: error: Option, profilelocking, does not take an argument ($enableval)." 1>&5; exit 1; }
  fi
