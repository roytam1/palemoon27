if test -n "$MACOSX_DEPLOYMENT_TARGET" -a -n "$MOZ_RUST"; then
  echo $ac_n "checking if we're targeting 32-bit""... $ac_c" 1>&6
echo "configure:9794: checking if we're targeting 32-bit" >&5
  if test -z "$HAVE_64BIT_BUILD"; then
    echo "$ac_t""using $RUSTC" 1>&6
  else
    echo "$ac_t""no" 1>&6
  fi
  echo $ac_n "checking rustc compatibility with MacOS X""... $ac_c" 1>&6
echo "configure:9801: checking rustc compatibility with MacOS X" >&5
  # Stock rustc doesn't support MacOS X 10.6 or earlier.
  # https://github.com/rust-lang/rust/issues/25342
  _MACOSX_TARGET_MINOR=`echo "$MACOSX_DEPLOYMENT_TARGET" | cut -d. -f2`
  if test "$_MACOSX_TARGET_MINOR" -lt 7; then
            cat > conftest.rs <<EOF
      #[no_mangle]
      pub extern fn rusty_answer() -> u8 { 42 }
EOF
    ac_try="$RUSTC --crate-type staticlib -o conftest.a conftest.rs >/dev/null"
    { (eval echo configure:9811: \"$ac_try\") 1>&5; (eval $ac_try) 2>&5; }
    save_LDFLAGS=$LDFLAGS
    LDFLAGS="$LDFLAGS conftest.a -lpthread -lm"
    cat > conftest.$ac_ext <<EOF
#line 9815 "configure"
#include "confdefs.h"
/* Override any gcc2 internal prototype to avoid an error.  */
/* We use char because int might match the return type of a gcc2
    builtin and then its argument prototype would still apply.  */
char rusty_answer();

int main() {
rusty_answer()
; return 0; }
EOF
