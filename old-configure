if test "${with_gl_provider+set}" = set; then
  withval="$with_gl_provider"
   val=`echo $withval`
    MOZ_GL_PROVIDER="$val"
fi


if test -n "$MOZ_GL_PROVIDER"; then
MOZ_GL_DEFAULT_PROVIDER=$MOZ_GL_PROVIDER

