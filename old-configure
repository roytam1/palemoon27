if test -n "${WEBP_DIR}" -a "${WEBP_DIR}" != "yes"; then
    CFLAGS="-I${WEBP_DIR}/include $CFLAGS"
    LDFLAGS="-L${WEBP_DIR}/lib $LDFLAGS"
fi
if test -z "$WEBP_DIR" -o "$WEBP_DIR" = no; then
    MOZ_NATIVE_WEBP=
else
    echo $ac_n "checking for WebPGetDecoderVersion in -lwebp""... $ac_c" 1>&6
echo "configure:16982: checking for WebPGetDecoderVersion in -lwebp" >&5
ac_lib_var=`echo webp'_'WebPGetDecoderVersion | sed 'y%./+-%__p_%'`
if eval "test \"`echo '$''{'ac_cv_lib_$ac_lib_var'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  ac_save_LIBS="$LIBS"
LIBS="-lwebp  $LIBS"
cat > conftest.$ac_ext <<EOF
#line 16990 "configure"
#include "confdefs.h"
/* Override any gcc2 internal prototype to avoid an error.  */
/* We use char because int might match the return type of a gcc2
    builtin and then its argument prototype would still apply.  */
char WebPGetDecoderVersion();

int main() {
WebPGetDecoderVersion()
; return 0; }
EOF
if { (eval echo configure:17001: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
  rm -rf conftest*
  eval "ac_cv_lib_$ac_lib_var=yes"
else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
  rm -rf conftest*
  eval "ac_cv_lib_$ac_lib_var=no"
fi
rm -f conftest*
LIBS="$ac_save_LIBS"

fi
