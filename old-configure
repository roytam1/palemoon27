            CAIRO_TEE_LIBS="`$PKG_CONFIG --libs \"cairo-tee >= $CAIRO_VERSION\" |sed s/-Wl,--export-dynamic//g`"
            echo "$ac_t""$CAIRO_TEE_LIBS" 1>&6
        else
            CAIRO_TEE_CFLAGS=""
            CAIRO_TEE_LIBS=""
            ## If we have a custom action on failure, don't print errors, but 
