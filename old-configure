    { echo "configure: error: The program keytool was not found.  Set \$JAVA_HOME to your Java SDK directory or use --with-java-bin-path={java-bin-dir}" 1>&2; echo "configure: error: The program keytool was not found.  Set \$JAVA_HOME to your Java SDK directory or use --with-java-bin-path={java-bin-dir}" 1>&5; exit 1; }
  fi

  echo $ac_n "checking for minimum required javac version >= 1.7""... $ac_c" 1>&6
echo "configure:23038: checking for minimum required javac version >= 1.7" >&5

    _javac_version=$($JAVAC -version 2>&1 | cut -d ' ' -f 2)

      _javac_major_version=$(echo $_javac_version | cut -d '.' -f 1)
  _javac_minor_version=$(echo $_javac_version | cut -d '.' -f 2)

  echo "$ac_t""$_javac_version" 1>&6

    if test "$_javac_major_version" -ne "1" -o \
       \( "$_javac_minor_version" -lt "7" \); then
      { echo "configure: error: javac 1.7 or higher is required." 1>&2; echo "configure: error: javac 1.7 or higher is required." 1>&5; exit 1; }
  fi
fi


