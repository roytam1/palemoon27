done
test -n "$HOST_CC" || HOST_CC=""""

if test -z "$HOST_CC"; then
    { echo "configure: error: no acceptable c compiler found in \$PATH" 1>&2; echo "configure: error: no acceptable c compiler found in \$PATH" 1>&5; exit 1; }
