  withval="$with_l10n_base"
  L10NBASEDIR=$withval
fi

if test -n "$L10NBASEDIR"; then
    if test "$L10NBASEDIR" = "yes" -o "$L10NBASEDIR" = "no"; then
        { echo "configure: error: --with-l10n-base must specify a path" 1>&2; echo "configure: error: --with-l10n-base must specify a path" 1>&5; exit 1; }
    elif test -d "$L10NBASEDIR"; then
        L10NBASEDIR=`cd "$L10NBASEDIR" && pwd -P`
    else
        { echo "configure: error: Invalid value --with-l10n-base, $L10NBASEDIR doesn't exist" 1>&2; echo "configure: error: Invalid value --with-l10n-base, $L10NBASEDIR doesn't exist" 1>&5; exit 1; }
    fi
fi


